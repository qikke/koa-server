dth: '100%', height: '100%' }}>\n        <Menu\n          style={{ fontSize: 13.5, backgroundColor: '#000' }}\n          mode='horizontal'\n          defaultSelectedKeys={[this.matchRouteName]}\n        >\n          <Menu-Item\n            style={{\n              marginLeft: 24,\n              marginRight: 30,\n              fontSize: 18,\n              textAlign: 'center',\n              color: '#fff important',\n              float: 'left'\n            }}\n          >\n            <a href={'/'} className='hover-scale logo-text' style={{\n              color: '#fff2e8'\n            }}>标题</a>\n          </Menu-Item>\n          {\n            navRouters.map((e, i) => (\n              <Menu.Item key={e.name}>\n                {\n                  getMenuContent({ ...e })\n                }\n              </Menu.Item>\n            ))\n          }\n        </Menu>\n        <Spin\n          spinning={loading}\n          tip={tip}\n          wrapperClassName='content-spin full'\n        >\n          {children}\n        </Spin>\n      </div>\n    )\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"cb06ee5677770a990a63b4ebbc810aa2","cacheData":{"env":{}}}